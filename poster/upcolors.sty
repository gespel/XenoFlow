\RequirePackage{xcolor}
\selectcolormodel{cmyk}
% Color definitions from preliminary style guide
% Names are made up but logical, add UP prefix as a safeguard for
% uniqueness.
\definecolor{UPblue}{cmyk}{1, 0.7, 0.1, 0.5}
\definecolor{UPlightblue}{cmyk}{1, 0.3, 0.15, 0}
\definecolor{UPviolet}{cmyk}{0.8, 0.99, 0, 0}
\definecolor{UPpink}{cmyk}{0.4, 1, 0.19, 0.11}
\definecolor{UPgreen}{cmyk}{0.78, 0.11, 1, 0}
\definecolor{UPyellow}{cmyk}{0.18, 0.38, 1, 0.07}
\definecolor{UPorange}{cmyk}{0, 0.7, 0.95, 0}
\definecolor{UPred}{cmyk}{0.25, 1, 1, 0.26}

\definecolor{UPprimary}{named}{UPblue}
\definecolor{UPsecondary}{named}{UPlightblue}

%% text colors
\definecolor{UPtextBlack}{named}{black}
\definecolor{UPtextGray}{gray}{.2}

\definecolor{UPtextprimary}{named}{UPtextBlack}
\definecolor{UPtextsecondary}{named}{UPtextGray}

%% shades of gray used in previous poster template (2019):
\definecolor{UPGray}{cmyk}{0.34, 0.17, 0, 0.61}  
\definecolor{UPGray2}{cmyk}{0.36, 0.16, 0, 0.41}
\definecolor{UPGray3}{cmyk}{0.23, 0.17, 0.18, 0.01}
